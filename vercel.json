{
    "version": 2,
    "builds": [
      {
        "src": "app.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app.py"
      }
    ],
    "functions": {
      "api/weekly_report.py": {
        "runtime": "python3.9"
      }
    },
    "cron": [
      {
        "path": "/api/weekly_report.py",
        "schedule": "*/5 * * * *"  
      }
    ]
  }